<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/village_all_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="14dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/village_all_city"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_id"
        app:layout_constraintTop_toBottomOf="@+id/village_all_id"/>

    <TextView
        android:id="@+id/village_all_sheng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/village_all_city"
        app:layout_constraintTop_toTopOf="@+id/village_all_id"/>

    <TextView
        android:id="@+id/village_all_shi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_sheng"
        app:layout_constraintTop_toBottomOf="@+id/village_all_sheng"
        android:layout_marginLeft="0dp"/>

    <TextView
        android:id="@+id/village_all_xian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/village_all_shi"
        app:layout_constraintTop_toTopOf="@+id/village_all_shi"/>

    <TextView
        android:id="@+id/village_all_look"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:autoLink="all"
        android:text="@string/to_edit"
        android:textColor="@color/app_bg"
        app:layout_constraintBottom_toBottomOf="@+id/village_all_shi"
        app:layout_constraintRight_toLeftOf="@+id/village_all_delete"
        app:layout_constraintTop_toTopOf="@+id/village_all_sheng"
        app:layout_constraintVertical_bias="0.555"/>

    <ImageView
        android:id="@+id/village_all_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="7dp"
        android:layout_marginTop="0dp"
        android:src="@drawable/sc"
        app:layout_constraintBottom_toBottomOf="@+id/village_all_look"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/village_all_look"
        app:layout_constraintVertical_bias="0.6"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="14dp"
        android:background="@color/gray_ee"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/village_all_shi"/>

</android.support.constraint.ConstraintLayout>