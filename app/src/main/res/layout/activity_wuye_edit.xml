<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.superc.shangjiaban.ui.activity.WuyeEditActivity">


    <TextView
        android:id="@+id/wuye_edit_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textColor="@color/black"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:layout_marginStart="16dp"
        android:text="用户名"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_name"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:layout_marginLeft="0dp"
        android:text="佣金"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_money"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_tv"/>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:layout_marginLeft="0dp"
        android:text="联系人"
        app:layout_constraintLeft_toLeftOf="@+id/textView16"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_username"/>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:textColor="@color/black"
        android:text="电话"
        app:layout_constraintLeft_toLeftOf="@+id/textView18"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_phone"/>

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="-1dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18"/>

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/textView20"/>

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dp"
        android:text="Email地址"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView27"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_youxiang"/>

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView32"
        app:layout_constraintTop_toTopOf="@+id/textView32"/>

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="收款途径"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView33"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_shoukuantujing"/>

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="@+id/textView34"/>

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dp"
        android:text="收款账号"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView35"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_shoukaunzhanghao"/>

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView36"
        app:layout_constraintTop_toTopOf="@+id/textView36"/>

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="新密码"
        android:visibility="invisible"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView37"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_newpwd"/>

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:visibility="invisible"
        android:textColor="@color/black"
        android:text="确认新密码"
        app:layout_constraintLeft_toLeftOf="@+id/textView38"
        app:layout_constraintBaseline_toBaselineOf="@+id/wuye_edit_surepwd"/>

    <EditText
        android:id="@+id/wuye_edit_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:enabled="false"
        android:background="@color/gray_ee"
        android:hint="用户名"
        android:padding="@dimen/padding_big"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/textView39"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <EditText
        android:id="@+id/wuye_edit_money"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_ee"
        android:padding="@dimen/padding_big"
        android:text="20"
        android:enabled="false"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_name"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_name"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_name"/>

    <EditText
        android:id="@+id/wuye_edit_username"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_ee"
        android:padding="@dimen/padding_big"
        android:text="小明"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_money"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_money"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_money"/>

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:maxLength="11"
        android:inputType="number"
        android:background="@color/gray_ee"
        android:padding="@dimen/padding_big"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_username"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_username"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_username"
        android:id="@+id/wuye_edit_phone"/>

    <EditText
        android:id="@+id/wuye_edit_youxiang"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_ee"
        android:hint="邮箱地址"
        android:padding="@dimen/padding_big"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_phone"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_phone"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_phone"/>

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/txt_noramal_size"
        android:background="@color/gray_ee"
        android:padding="@dimen/padding_big"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_youxiang"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_youxiang"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_youxiang"
        android:id="@+id/wuye_edit_shoukuantujing"/>
    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/txt_noramal_size"
        android:background="@color/gray_ee"
        android:padding="@dimen/padding_big"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_shoukuantujing"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_shoukuantujing"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_shoukuantujing"
        android:id="@+id/wuye_edit_shoukaunzhanghao"/>

    <EditText
        android:id="@+id/wuye_edit_newpwd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_ee"
        android:hint="留空表示不修改"
        android:visibility="invisible"
        android:padding="@dimen/padding_big"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_shoukaunzhanghao"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_shoukaunzhanghao"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_shoukaunzhanghao"
        app:layout_constraintHorizontal_bias="0.0"/>

    <EditText
        android:id="@+id/wuye_edit_surepwd"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_ee"
        android:hint="留空表示不修改"
        android:visibility="invisible"
        android:padding="@dimen/padding_big"
        android:textSize="@dimen/txt_noramal_size"
        app:layout_constraintLeft_toLeftOf="@+id/wuye_edit_newpwd"
        app:layout_constraintRight_toRightOf="@+id/wuye_edit_newpwd"
        app:layout_constraintTop_toBottomOf="@+id/wuye_edit_newpwd"/>

    <TextView
        android:id="@+id/wy_tv_complete"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="完成"
        android:padding="@dimen/padding_big"
        android:textColor="@color/white"
        android:gravity="center"
        android:background="@drawable/circle_appbg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>


</android.support.constraint.ConstraintLayout>