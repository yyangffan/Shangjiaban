<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/village_all_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="14dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/village_all_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_id"
        app:layout_constraintTop_toBottomOf="@+id/village_all_id"/>

    <TextView
        android:id="@+id/village_all_sheng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/feedback_username"
        app:layout_constraintTop_toTopOf="@+id/village_all_id"/>

    <TextView
        android:id="@+id/village_all_shi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_sheng"
        app:layout_constraintTop_toBottomOf="@+id/village_all_sheng"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="14dp"
        android:background="@color/gray_ee"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedback_content"/>

    <TextView
        android:id="@+id/feedback_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="7dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_shi"
        app:layout_constraintRight_toLeftOf="@+id/feedback_xiangqing"
        app:layout_constraintTop_toBottomOf="@+id/village_all_shi"/>

    <TextView
        android:id="@+id/feedback_username"
        android:layout_width="100dp"
        android:lines="1"
        android:ellipsize="end"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="12dp"
        android:drawableLeft="@drawable/xm"
        android:text="小花"
        android:textColor="@color/god_yellow"
        app:layout_constraintLeft_toLeftOf="@+id/village_all_city"
        app:layout_constraintTop_toBottomOf="@+id/village_all_city"/>

    <TextView
        android:id="@+id/feedback_xiangqing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:autoLink="all"
        android:text="@string/tv_xiangqing"
        android:textColor="@color/app_bg"
        app:layout_constraintBaseline_toBaselineOf="@+id/feedback_content"
        app:layout_constraintRight_toRightOf="parent"/>

</android.support.constraint.ConstraintLayout>